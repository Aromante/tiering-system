{
  "name": "tiering-system",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": { "node": ">=20" },
  "scripts": {
    "predev": "npm run install:backend",
    "dev": "concurrently -k -n backend,frontend \"npm --prefix backend run dev\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "install:backend": "npm --prefix backend install",
    "build": "vite build",
    "preview": "vite preview"
